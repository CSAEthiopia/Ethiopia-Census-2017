	reenter SUPERVISOR_MAIN_MENU;
endif;
numeric keb = tonumber($[1:3]);
numeric enumArea = tonumber($[4:2]);
numeric staff = tonumber($[6:10]);
launchListing(keb,enumArea, staff);

PROC SUPERVISOR_LAUNCH_HOUSEHOLD_QUES
