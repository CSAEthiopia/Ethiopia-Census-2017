{Application 'MENU' logic file generated by CSPro}
PROC GLOBAL
array codes(100);
array string stringCodes(100);
array string labels(100); 
numeric totalStaff;
file pffFile;

//Generate the EA code from kebele and EA
function string getEACode(keb, enumArea)
    getEACode = maketext("%02d%02d%02d%d%02d%02d%02d%03d%02d",
         SA_ID101,SA_ID102,SA_ID103,SA_ID104,SA_ID105,SA_ID106,
         SA_ID107,keb,enumArea);
end;

//Generate the EA code for the currently logged in enumerator
function string getEnumEACode()
    getEnumEACode = getEACode(KEBELE_CODE,ENUMERATION_AREA_CODE);
end;

// Generate Listing file name from EA code and Staff code.
function string listingFileName(keb, ea, staffCode)
	listingFileName = maketext("L%s_%010d.dat",getEACode(keb, ea),staffCode);
end;

function string householdFileName(keb, ea, staffCode)
	householdFileName = maketext("H%s_%010d.dat",getEACode(keb, ea),staffCode);
end;
 
// Load the name of staff from staff file using staff ID
function string getStaffName(staffId)

	S_STAFF_ID = staffId;
	if loadcase(STAFF_DICT, S_STAFF_ID) = 1 then
		getStaffName = strip(S_NAME);
	else
		// If staff id is valid this will not happen
		getStaffName = "";
	endif;
end;

function launchListing(keb,enumArea, staff)

	setfile(pffFile,"./listing.pff",create);

	filewrite(pffFile,"[Run Information]");
	filewrite(pffFile,"Version=CSPro 6.3");
	filewrite(pffFile,"AppType=Entry");
	filewrite(pffFile,"Description="); // empty description so doesn't show on tab
	filewrite(pffFile,"[DataEntryInit]");

	filewrite(pffFile,"[Files]");
	filewrite(pffFile,"Application=%s","../Listing/Listing.ent");
	filewrite(pffFile,"InputData=../Data/%s",listingFileName(keb, enumArea, staff));
	filewrite(pffFile,"[Parameters]");
	filewrite(pffFile,"region=%d",SA_ID101);
	filewrite(pffFile,"zone=%d",SA_ID102);
	filewrite(pffFile,"woreda=%d",SA_ID103);
	filewrite(pffFile,"town=%d",SA_ID104);
	filewrite(pffFile,"subcity=%d",SA_ID105);
	filewrite(pffFile,"PSA=%d",SA_ID106);
	filewrite(pffFile,"SA=%d",SA_ID107);
	filewrite(pffFile,"kebele=%d",keb);
	filewrite(pffFile,"EA=%d",enumArea);		
	filewrite(pffFile,"Staff ID=%d",S_STAFF_ID);	
	close(pffFile);

	if execpff(filename(pffFile),stop) = 0 then
		errmsg("Fail to launch listing");
    endif;		
end;

function launchHHquest(keb, enumArea, staff, husn, hhsn)
setfile(pffFile,"./Household.pff",create);

	filewrite(pffFile,"[Run Information]");
	filewrite(pffFile,"Version=CSPro 6.3");
	filewrite(pffFile,"AppType=Entry");
	filewrite(pffFile,"Description="); // empty description so doesn't show on tab
	filewrite(pffFile,"[DataEntryInit]");
	
	string householdFName = pathname(application) + 
		maketext("..\Data\%s",householdFileName(keb, enumArea, staff));
	
	// Use modify mode if the case already exists in the household
	// data file, otherwise use add mode.
	string mode = "add";
	if fileexist(householdFName) then
		ID108 = keb;
		ID109 = enumArea;
		if setfile(HOUSEHOLD_DICT, householdFName) = 1 and
			loadcase(HOUSEHOLD_DICT, SA_ID101,SA_ID102,SA_ID103,
		 	SA_ID104,SA_ID105,SA_ID106,
         	SA_ID107,ID108,ID109) = 1 then
			mode = "modify";
		endif;
		close(HOUSEHOLD_DICT);
	endif;
	
	string caseIds = getEACode(keb, enumArea) + maketext("%03d%03d", husn,hhsn);
	filewrite(pffFile,"StartMode=%s;%s",mode, caseIds);	
	
	filewrite(pffFile,"[Files]");
	filewrite(pffFile,"Application=%s","../Household/Household.ent");
	filewrite(pffFile,"InputData=%s",householdFName);
	filewrite(pffFile,"[Parameters]");
	filewrite(pffFile,"region=%d",SA_ID101);
	filewrite(pffFile,"zone=%d",SA_ID102);
	filewrite(pffFile,"woreda=%d",SA_ID103);
	filewrite(pffFile,"town=%d",SA_ID104);
	filewrite(pffFile,"subcity=%d",SA_ID105);
	filewrite(pffFile,"PSA=%d",SA_ID106);
	filewrite(pffFile,"SA=%d",SA_ID107);
	filewrite(pffFile,"kebele=%d",keb);
	filewrite(pffFile,"EA=%d",enumArea);		
	filewrite(pffFile,"Staff ID=%d",staff);
	filewrite(pffFile,"Housing SN=%d",husn);		
	filewrite(pffFile,"Household SN=%d",hhsn);	
	close(pffFile);

	if execpff(filename(pffFile),stop) = 0 then
		errmsg("Fail to launch Household form");
    endif;		
   
end;

// Build dynamic value set from assignment dictionary
// where labels show Kebele and EA codes and
// values are kebele and EA concatenated.
function makeAssignedEAValueSet()
	numeric i=1;
	locate(ASSIGNMENT_DICT,>=,"");
	while loadcase(ASSIGNMENT_DICT) do
		stringCodes(i)=maketext("%03d%02d%010d", A_ID108, A_ID109, A_STAFF_ID);
		labels(i)=maketext("Kebele %03d EA %02d: (%s)",A_ID108,A_ID109, getStaffName(A_STAFF_ID));
		i=i+1;
	enddo;
	stringCodes(i) = "back";
	labels(i) = "Back to Main menu";
	i = i+1;	
	stringCodes(i)="";
end;

